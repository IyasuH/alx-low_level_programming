#include <stdio.h>
#include <stdlib.h>
#include "dog.h"

/**
 *init_dog - function
 *struct dog - structure
 *@d: pointer to the structure
 */

void print_dog(struct dog *d)
{
if (d != NULL)
{
  if(d->name != NULL)
printf("Name: %s\n", d->name);
  else
    printf("Name: (nil)\n");
printf("Age: %f", d->age);
  if(d->owner != NULL)
printf("Owner: %s\n", d->owner);
  else
    printf("Owner: (nil)\n");
}
}
